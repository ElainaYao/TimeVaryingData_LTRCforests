class(LTRCRSFobj)
devtools::build_manual(pkg = ".", path = NULL)
N  = 300
Distribution = "WI"
censor.rate = 1
num.pseu = 11
ll = 1
######################### ==== interaction model ======== ###############
model = 4 ## interaction
## Create the dataset
set.seed(101)
sampleID = sample(10000000,500)
set.seed(sampleID[ll])
if (model == 2){
RET <- Timevarying_linear_gnrt(N=N, Distribution=Distribution, censor.rate=censor.rate, npseu = num.pseu, partial = TRUE)
rtype = "linear"
} else if (model == 3) {
RET <- Timevarying_nonlinear_gnrt(N=N, Distribution=Distribution, censor.rate=censor.rate, npseu = num.pseu, partial = TRUE)
rtype = "nonlinear"
} else if (model == 4) {
RET <- Timevarying_itct_gnrt(N=N, Distribution=Distribution, censor.rate=censor.rate, npseu = num.pseu, partial = TRUE)
rtype = "interaction"
} else {
stop("Wrong model type is used.")
}
